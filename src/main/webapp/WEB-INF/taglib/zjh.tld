    <tag>
        <description>Based on the permission of display or hidden content</description>
        <name>sum</name>
        <tag-class>com.frame.core.taglib.Sum</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description></description>
            <name>qm</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description></description>
            <name>conditions</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description></description>
            <name>sumField</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description></description>
            <name>label</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
