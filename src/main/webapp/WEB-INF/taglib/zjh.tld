        </attribute>
        <attribute>
            <name>pageIndexStyle</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <description>pc:page target request param</description>
        <name>pageParam</name>
        <tag-class>com.frame.core.taglib.PageParam</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <description>request param name</description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description>request param value</description>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>


    <tag>
        <description>this is a ui tag for adjunct</description>
        <name>adjunct2</name>
        <tag-class>com.frame.core.taglib.Adjunct2Tag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
